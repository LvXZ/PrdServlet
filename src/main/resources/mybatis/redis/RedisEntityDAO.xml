<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.prd.module.redis.dao.RedisEntityDao">
  <select id="queryRedisEntityByEmployeeID" parameterType="int" resultType="com.prd.module.redis.entity.RedisEntity">


    SELECT *
    FROM tb_redis
    WHERE employee_id = #{employeeID}


  </select>
</mapper>